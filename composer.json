{
    "name": "squadms/rules",
    "description": "The SquadMS Rules Management module.",
    "keywords": ["laravel", "squad", "squadms", "rules"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Patrick Henninger",
            "email": "privat@skyraptor.eu",
            "homepage": "https://skyraptor.eu/"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0",
        "nesbot/carbon": "^2.0",
        "filament/spatie-laravel-translatable-plugin": "^2.0",
        "spatie/eloquent-sortable": "^4.0",
        "squadms/foundation": "dev-navigation"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SquadMS\\Rules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SquadMS\\Rules\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SquadMS\\Rules\\RulesServiceProvider",
                "SquadMS\\Rules\\Providers\\FilamentServiceProvider",
                "SquadMS\\Rules\\Providers\\ModulesServiceProvider",
                "SquadMS\\Rules\\Providers\\PermissionsServiceProvider",
                "SquadMS\\Rules\\Providers\\RouteServiceProvider"
            ]
        }
    }
}
